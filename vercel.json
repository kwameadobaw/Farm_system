{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_SECRET_KEY": "your-secret-key-here",
    "ADMIN_USERNAME": "admin",
    "ADMIN_PASSWORD": "password123"
  }
}